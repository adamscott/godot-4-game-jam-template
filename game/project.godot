; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Godot 4 Game Jam Template"
config/description="Godot 4 Game Jam Template"
run/main_scene="res://main/main.tscn"
config/features=PackedStringArray("4.0")
boot_splash/bg_color=Color(0.141176, 0.141176, 0.141176, 1)
boot_splash/show_image=false

[autoload]

Config="*res://autoload/config/config.tscn"
AudioSettings="*res://autoload/settings/audio/audio_settings.tscn"
DisplaySettings="*res://autoload/settings/display/display_settings.tscn"
Game="*res://autoload/game/game.tscn"
BeehaveGlobalMetrics="*res://addons/beehave/metrics/beehave_global_metrics.gd"

[editor_plugins]

enabled=PackedStringArray("res://addons/imjp94.yafsm/plugin.cfg", "res://addons/gdUnit4/plugin.cfg", "res://addons/beehave/plugin.cfg")
